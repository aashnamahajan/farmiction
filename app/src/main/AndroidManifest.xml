<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aashna.farmiction">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup" />
        <activity android:name=".Homescreen" />
        <activity android:name=".Mycrops" />
        <activity android:name=".CropLibrary" />
        <activity android:name=".Rice" />
        <activity android:name=".Carrot" />
        <activity android:name=".Sugarcane" />
        <activity android:name=".Wheat" />
        <activity android:name=".Weather" />
        <activity android:name=".grow_select_crop"></activity>
    </application>

</manifest>